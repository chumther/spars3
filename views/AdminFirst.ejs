<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title>SPARS | Welcome</title>

  <!--Bootstra cdn -->
  <script src="https://kit.fontawesome.com/46ca99fbcf.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/master.css">

<body>
  <nav class="navbar navbar-dark sticky-top bg-dark flex-md-nowrap p-0">
    <a class="navbar-brand col-sm-3 col-lg-2 mr-0" href="#">
      <img src="/imgs/origami.png" height="25px" alt="">
      SPARS
    </a>
    <ul class="navbar-nav px-3">
      <li class="nav-item text-nowrap">
        <a class="nav-link pr-4" href="#" onclick="SignOut()">Salir</a>
      </li>
    </ul>
  </nav>

  <div class="container-fluid">
    <div class="row">
      <nav class="col-lg-2 col-sm-3 bg-light sidebar">
        <div class="sidebar-sticky">

          <h6 class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
            <span>Notificaciones</span>
          </h6>
          <ul class="nav flex-column mb-2">
            <li class="nav-item">
            </li>
          </ul>
        </div>
      </nav>

      <main role="main" class="col-lg-10 col-sm-9 ml-auto pt-3 px-4">

        <div class="container pb-2 my-3 border-bottom">
          <h1 id="namesTag">Buscar</h1>
        </div>

        <div class="container pb-2 my-4">

          <div class="row">
            <div class="col-lg-6 col-sm-12 mb-4">
              <div class="card h-100">
                <div class="card-header">
                  <span class="h3">Nombres y Apellidos</span>
                </div>
                <div class="card-body">
                  <form class="" action="/search" method="post">
                    <div class="form-group">
                      <label for="exampleInputEmail1">Apellidos</label>
                      <input type="text" class="form-control" name="apellidosInput">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputEmail1">Nombres</label>
                      <input type="text" class="form-control" name="nombresInput">
                      <input type="text" name="user" id="namesuid" value="" hidden>
                      <input type="text" name="searchType" id="searchType" value="names" hidden>
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                  </form>
                </div>
              </div>
            </div>

            <div class="col-lg-6 col-sm-12 mb-4">
              <div class="card h-100">
                <div class="card-header">
                  <span class="h3">Numero de Identificacion</span>
                </div>
                <div class="card-body">
                  <form class="" action="/search" method="post">
                    <div class="form-group">
                      <label for="exampleInputEmail1"># ID</label>
                      <input type="number" class="form-control" name="IdInput">
                      <input type="text" name="user" id="iduid" value="1234567890" hidden>
                      <input type="text" name="searchType" id="searchType" value="id" hidden>
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
    </div>
  </div>
  </div>
  <!-- Bootstrap JS -->
  <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>

  <!-- The core Firebase JS SDK is always required and must be listed first -->
  <script src="https://www.gstatic.com/firebasejs/7.9.1/firebase-app.js"></script>
  <script src="https://www.gstatic.com/firebasejs/7.10.0/firebase-auth.js"></script>

  <script src="scripts/adminFirst.js" charset="utf-8"></script>
  <script src="scripts/postRedirect.js" charset="utf-8"></script>
</body>

</html>
